{
  "manifest_version": 3,
  "name": "LAPS Password Font Override", 
  "version": "1.0",
  "description": "Change to LAPS password font in Intune to a monospace font", 
  "permissions": ["activeTab", "storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"], 
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Toggle Font Override"
  }
}
